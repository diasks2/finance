<%= form_for @account do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :owner %>
  <%= f.text_field :owner %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :address %>
  <%= f.text_area :address %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <br>
  <br>
  <%= f.submit "Create New Account" %>

<% end %>